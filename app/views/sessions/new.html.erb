<h1>Login</h1>
<div class="container">
  <%= bootstrap_form_for :user do |f| %>
    <%= f.text_field :name, placeholder: "Enter Name" %>
    <%= f.text_field :email, placeholder: "Enter Email" %>
    <%= f.password_field :password, placeholder: "Enter Password" %>
    <%= f.submit "Login" %>
  <% end %>
</div>